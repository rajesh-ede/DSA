
 Kadane's Algorithm

class Main{
    public static int Max(int arr[]){
        int n = arr.length;
        int max = arr[0];
        int maxso = arr[0];
        for(int i = 1; i < n; i++){
            max = Math.max(arr[i], max+arr[i]);
            maxso = Math.max(max,maxso);
        }
        return maxso;
    }
    public static void main(String args[]){
        int arr[]= {-2, 1, -3, 4, -1, 2, 1, -5, 4};
        System.out.println(Max(arr));
    }
}
